<ApiRequest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <ApiKey>{{ apiKey }}</ApiKey>
    <Data>
        <Source>
            <Url>ftp://ftp.expertsender.com/Lists/test.csv</Url>
            <Username>username</Username>
            <Password>password</Password>
        </Source>
        <Target>
            <Name>API test import </Name>
            <SubscriberList>80</SubscriberList>
        </Target>
        <ImportSetup>
            <Mode>AddAndUpdate</Mode>
            <Delimiter>,</Delimiter>
            <Quote>"</Quote>
            <Escape>"</Escape>
            <Comment>|</Comment>
            <Encoding>UTF-8</Encoding>
            <StartingLine>1</StartingLine>
            <AllowImportingUnsubscribed>false</AllowImportingUnsubscribed>
            <AllowImportingRemovedByUi>true</AllowImportingRemovedByUi>
            <Mapping>
                <Column>
                    <Number>1</Number>
                    <Field>Firstname</Field>
                </Column>
                <Column>
                    <Number>2</Number>
                    <Field>Lastname</Field>
                </Column>
                <Column>
                    <Number>3</Number>
                    <Field>Email</Field>
                </Column>
                <Column>
                    <Number>4</Number>
                    <Property>94</Property>
                </Column>
                <Column>
                    <Number>5</Number>
                    <Field>age</Field>
                </Column>
            </Mapping>
        </ImportSetup>
    </Data>
</ApiRequest>