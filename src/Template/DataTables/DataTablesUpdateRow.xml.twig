<ApiRequest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <ApiKey>{{ apiKey }}</ApiKey>
    <TableName>{{ tableName }}</TableName>
    {% if conditions is defined and conditions %}
        <PrimaryKeyColumns>
            {% for c,v in conditions %}
                <Column>
                    <Name>{{ c }}</Name>
                    <Value>{{ v }}</Value>
                </Column>
            {% endfor %}
        </PrimaryKeyColumns>
    {% endif %}
    {% if data is defined and data %}
        <Columns>
            {% for c,v in data %}
                <Column>
                    <Name>{{ c }}</Name>
                    <Value>{{ v }}</Value>
                </Column>
            {% endfor %}
        </Columns>
    {% endif %}
</ApiRequest>