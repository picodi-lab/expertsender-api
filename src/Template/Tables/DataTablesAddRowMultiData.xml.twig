<ApiRequest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <ApiKey>{{ apiKey }}</ApiKey>
    <TableName>{{ tableName }}</TableName>
    {% if MultiData is defined and MultiData %}
    <MultiData>
        {% for row in MultiData %}
        <Row>
            <Columns>
                {% for c,v in row %}
                    <Column>
                        <Name>{{ c }}</Name>
                        <Value>{{ v }}</Value>
                    </Column>
                {% endfor %}
            </Columns>
        </Row>
        {% endfor %}
    </MultiData>
    {% endif %}
</ApiRequest>